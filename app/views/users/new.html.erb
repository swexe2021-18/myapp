<%= form_for @user, url: {action: :create} do |f| %>
  <%= f.label :uid, 'uid' %> 
  <%= f.text_field :uid %> 
  <%= f.label :password, 'password' %> 
  <%= f.password_field :pass %> 
  <%= f.label :password_confirmation, 'password(確認)' %> 
  <%= f.password_field :password %> 
  <%= f.submit '登録' %>
<% end %>